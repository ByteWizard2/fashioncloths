<body>
    <div class="screen-overlay"></div>
    <aside class="navbar-aside" id="offcanvas_aside">
        <div class="aside-top">
            <a href="/api/v1" class="brand-wrap">
                <img src="~/assets/img/logo.png" class="logo" alt="UrbaneWardrobe Dashboard" />
                <h4>Dashboard</h4>
            </a>
            <div>
                <button class="btn btn-icon btn-aside-minimize"><i class="text-muted material-icons md-menu_open"></i></button>
            </div>
        </div>
        <nav>
            <ul class="menu-aside">
                <li class="menu-item">
                    <a class="menu-link" disabled>
                        <i class="icon material-icons md-home"></i>
                        <span class="text"> Hi,</span>
                    </a>
                </li>
                <li class="menu-item ">
                    <a class="menu-link"  asp-controller="UserProfile" asp-action="Profile">
                        <i class="icon material-icons md-account_circle"></i>
                        <span class="text">Profile</span>
                    </a>
                </li>
                <li class="menu-item ">
                    <a class="menu-link" asp-controller="UserProfile" asp-action="AddressView">
                        <i class="icon material-icons md-location_on"></i>
                        <span class="text">Address</span>
                    </a>
                </li>
                <li class="menu-item ">
                    <a class="menu-link" asp-controller="UserProfile" asp-action="OrderList">
                        <i class="icon material-icons md-receipt"></i>
                        <span class="text">Orders</span>
                    </a>
                </li>
                <li class="menu-item ">
                    <a class="menu-link" asp-controller="UserProfile" asp-action="WalletView">
                        <i class="icon material-icons md-account_balance_wallet"></i>
                        <span class="text">Wallet</span>
                    </a>
                </li>
            </ul>
            <hr />
            <ul class="menu-aside">
                <li class="menu-item">
                    <a asp-action="Logout" asp-controller="Home" class="menu-link">
                        <i class="icon material-icons md-exit_to_app"></i>
                        <span class="text">Logout</span>
                    </a>
                </li>
            </ul>
            <br />
            <br />
        </nav>
    </aside>


    @*  {{!-- header --}} *@
    <div class="main-wrap">
        <nav class="navbar navbar-expand-lg bg-white">
            <div class=" container-fluid">
                <button class="navbar-toggler ms-auto" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse justify-content-lg-center" id="navbarNavAltMarkup">
                    <div class="navbar-nav">
                        <a class="nav-link active" asp-controller="UserProfile" asp-action="Profile">Home</a>

                        <a class="nav-link" asp-controller="Home" asp-action="MainHome">Shop</a>

                        @*      <a class="nav-link" href=""></a> *@

                        <a class="nav-link" href="#">Contact</a>
                    </div>
                </div>
                <ul class="header__right__widget d-flex ms-3 ">
                    <li>
                        <a href="/api/v1/users/wishlist">
                            <span class="icon_heart_alt"></span>
                            <div class="tip" id="wishlist-tip"></div>
                        </a>
                    </li>
                    <li>
                        <a href="/api/v1/users/cart">
                            <span class="icon_bag_alt "></span>
                            <div class="tip " id="cart-tip"></div>
                        </a>
                    </li>
                </ul>
            </div>
        </nav>
    </div>



  

    <div class="">
        <main role="main" class="pb-3">
            @RenderBody()

            <partial name="_Notification" />
        </main>
    </div>


    <script>

        document.addEventListener('DOMContentLoaded', function () {
            var menuItems = document.querySelectorAll('.menu-item');
            var currentURL = window.location.pathname;

            menuItems.forEach(function (item) {
                var itemURL = item.querySelector('a').getAttribute('href');
                if (currentURL.startsWith(itemURL)) {
                    item.classList.add('active');
                }
            });
        });
    </script>


