    <script src="~/Adassets/js/mixitup.min.js"></script>
    <script src="~/Adassets/js/jquery.countdown.min.js"></script>
    <script src="~/Adassets/js/jquery.slicknav.js"></script>
    <script src="~/Adassets/js/owl.carousel.min.js"></script>
    <script src="~/Adassets/js/jquery.nicescroll.min.js"></script>
    <script src="~/Adassets/js/main.js"></script>

   @*  {{!-- bootstrap --}} *@
